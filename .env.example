# Environment Configuration for Deployment
# Copy this to .env and fill in your values

# Your private key
PRIVATE_KEY=0x1234567890abcdef...  # Your actual private key

# Chainlink VRF Subscription ID (get from vrf.chain.link)
VRF_SUBSCRIPTION_ID=12345  # Your actual subscription ID

# RPC URL for Sepolia
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Custom addresses (leave empty to use deployer address)
# For testing on Sepolia, leave these empty - deployer will be used for all roles
TREASURY_ADDRESS=
VAULT_ADDRESS=
EMERGENCY_ADMIN=

# For contract verification on Etherscan
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY